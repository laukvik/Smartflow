{
  "plugins": [],
  "source": {
    "include": ["./src"],
    "includePattern": ".+\\.js(doc|x)?$"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": false
    }
  },
  "opts": {
    "template": "templates/default",
    "encoding": "utf8",
    "destination": "./dist/docs",

    "recurse": true
  }
}
